{
	"app": {
		"name": "Migraine Tracker",
		"slug": "migraine-tracker",
		"entry_name": "migraine",
		"description": "migraine tracker",
		"private": false,
		"icon": "ðŸ§ ",
		"color": "#202124",
		"storageType": "",
		"storage": "",
		"charts_placement": "",
		"entries_name": "migraines"
	},
	"temporal": {
		"times": "yes",
		"ranges": "yes"
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				]
			},
			"name": "date"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " at ",
				"type": "time",
				"default": "[time($startup, 'hours')]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(date, time)"
			},
			"name": "date_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " to ",
				"type": "time",
				"default": "",
				"visible": "[time]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "end_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"prefix": " on ",
				"visible": "[if(time and end_time, if(end_date = date, 'edit', true), false)]",
				"default": "[if(end_time, if(end_time < time, date(date + 1 * days()), date))]",
				"min_date": "[date]"
			},
			"name": "end_date"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(end_date, end_time)"
			},
			"name": "end_date_time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "end_date_time - date_time"
			},
			"name": "duration"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"visible": "[end_date and (time and end_time)]",
				"prefix": "(",
				"suffix": ")",
				"expression": "duration(duration, 2)"
			},
			"name": "duration_readable"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "text",
				"appearance": "line",
				"multiline": true
			},
			"name": "notes"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"max": 5,
				"prefix": "Pain ",
				"appearance": "line"
			},
			"name": "pain"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"prefix": "Side of Head ",
				"allow_multiple": "",
				"type": "options",
				"options": "left, right, both",
				"appearance": ""
			},
			"name": "side"
		}
	],
	"root": [
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"target_preset": "new",
				"target": "add(entry)",
				"set_pairs": [],
				"filter_pairs": [],
				"label": "Add new Migraine"
			}
		},
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"prefix": "Time since last migraine ",
				"expression": "duration($now - last(date_time))"
			}
		}
	],
	"charts": [
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"aggregate": "average",
				"aggregate_of": "pain",
				"type": "bar",
				"orientation": "horizontal"
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"aggregate_of": "entry",
				"type": "table",
				"group_by": "side",
				"aggregate": "count"
			}
		}
	]
}